{
  "name": "prettier-sql-oracle",
  "version": "0.0.0",
  "description": "Prettier plugin for Oracle SQL",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "download-lexer": "curl https://raw.githubusercontent.com/antlr/grammars-v4/master/sql/plsql/PlSqlLexer.g4 > antlr4-grammars/PlSqlLexer.g4",
    "download-parser": "curl https://raw.githubusercontent.com/antlr/grammars-v4/master/sql/plsql/PlSqlParser.g4 > antlr4-grammars/PlSqlParser.g4",
    "download-lexer-base": " curl https://raw.githubusercontent.com/antlr/grammars-v4/master/sql/plsql/JavaScript/PlSqlBaseLexer.js > antlr4-grammars/PlSqlBaseLexer.js",
    "download-parser-base": "curl https://raw.githubusercontent.com/antlr/grammars-v4/master/sql/plsql/JavaScript/PlSqlBaseParser.js > antlr4-grammars/PlSqlBaseParser.js",
    "build-lexer": "java org.antlr.v4.Tool -Dlanguage=JavaScript -o antlr4-grammars antlr4-grammars/PlSqlLexer.g4",
    "build-parser": "java org.antlr.v4.Tool -Dlanguage=JavaScript -o antlr4-grammars antlr4-grammars/PlSqlParser.g4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cofad/prettier-sql-oracle.git"
  },
  "keywords": [
    "prettier",
    "plugin",
    "oracle",
    "sql"
  ],
  "author": "Will Warner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cofad/prettier-sql-oracle/issues"
  },
  "homepage": "https://github.com/cofad/prettier-sql-oracle#readme",
  "dependencies": {
    "antlr4": "^4.9.2",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
